import java.util.Scanner;
public class Series5
{
    public static void main(String[] args)
    {Scanner sc=new Scanner(System.in);
    int n=11;
    int a=sc.nextInt();
    for(int i=1;i<=a;i++)
    {
        int d=n*n;
        n+=4;
        System.out.printf("%d ",d);
        
    }
       
    }
}
