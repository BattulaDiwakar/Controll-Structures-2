import java.util.Scanner;
public class Series6
{
    public static void main(String[] args)
    {Scanner sc=new Scanner(System.in);
    int n=sc.nextInt();
    int a=0;
    for(int i=1;i<=n;i++)
    {
        if(i%2==0)
        { a=i*i-2;
        System.out.printf("%d ",a);
            
        }else
        {
            a=i*i-1;
System.out.printf("%d ",a);
}
    }
        
    }
}
