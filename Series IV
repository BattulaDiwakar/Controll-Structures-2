import java.util.Scanner;
public class Series4
{
    public static void main(String[] args)
    {
            double b=0.5,d;
            Scanner sc=new Scanner(System.in);
          int a=sc.nextInt();
    for(int i=1;i<=a;i++)
    { System.out.printf("%.1f ",b);
         d=b*3;
        
         b=d;
        
    }
    }
}
